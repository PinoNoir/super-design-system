  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "browserslist": [
    "defaults"
  ],
  "files": [
    "lib",
    "!lib/database",
    "!lib/docs"
  ],
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    "./component-styles": "./lib/index.css",
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "check-types": "tsc --noEmit --pretty --skipLibCheck",
    "build": "node scripts/build.js",
    "build:lib": "rollup -c",
    "build-storybook": "rimraf storybook-static && mkdir storybook-static && pnpm storybook build --stats-json ./storybook-static",
    "test-storybook": "test-storybook",
    "chromatic": "npx chromatic --config ./chromatic.config.json --project-token=chpt_93f5ce837febe02",
    "clean:lib": "rimraf ./lib",
    "dev": "storybook dev -p 6006",
    "test": "jest --config ./jest.config.ts",
    "test:watch": "jest --config ./jest.config.ts --watch",
    "test:debug": "jest --config ./jest.config.ts --detectOpenHandles --runInBand --no-cache",
    "test:env": "jest --config ./jest.config.ts --passWithNoTests --coverageReporters=cobertura",
    "test:coverage": "jest --config ./jest.config.ts --coverage",
    "test:sonar": "jest --config ./jest.config.ts --watchAll=false --coverage --testResultsProcessor jest-sonar-reporter",
    "sonar": "node sonarqube-scanner.js",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,md,mdx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx,md,mdx}\" --fix"
  },
