{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "isolatedModules": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "noUncheckedIndexedAccess": true,
    "paths": {
      "@stretto/apps/*": ["apps/react-sandbox/src/*"],
      "@stretto/packages/mui-themes/*": ["mui-themes/src/*"],
      "@stretto/packages/cds-ui/*": ["cds-ui/src/*"],
      "@stretto/packages/cds-tokens/*": ["cds-tokens/src/*"],
      "@stretto/packages/cds-styles/*": ["cds-styles/src/*"]
    }
  }
}
